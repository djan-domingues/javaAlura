package byteBank;

public class TestaMetodo {
	
	public static void main(String[] args) {
		
		Conta contaDoPaulo = new Conta();
		
		contaDoPaulo.saldo = 100;
		
		contaDoPaulo.deposita(1250);
		
		System.out.println(contaDoPaulo.saldo);
		
		contaDoPaulo.saca(1351);
		
		System.out.println(contaDoPaulo.saldo);
		
		
		
	}

}
